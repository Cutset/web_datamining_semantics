tion des pi\u00e8ces, peuvent ais\u00e9ment \u00eatre retrouv\u00e9es.","id_mon":"PA00117786","siecle":15,"lat_mon":"45.744611394","lng_mon":"4.85262250796","type":"monument_historique"},{"nom_mon":"maison des Gondi","adresse_mon":"15, 17 mont\u00e9e Saint-Barth\u00e9l\u00e9my","commune_mon":"Lyon","historique":null,"id_mon":"PA00117924","siecle":null,"lat_mon":"45.7643374158","lng_mon":"4.82656287202","type":"monument_historique"},{"nom_mon":"Maison","adresse_mon":"4 rue Saint-Jean","commune_mon":"Lyon","historique":null,"id_mon":"PA00117930","siecle":null,"lat_mon":"45.7641049424","lng_mon":"4.8279634023","type":"monument_historique"},{"nom_mon":"Eglise Saint-Just","adresse_mon":"39, 41 rue des Farges","commune_mon":"Lyon","historique":null,"id_mon":"PA00117799","siecle":16,"lat_mon":"45.7560192145","lng_mon":"4.82042219775","type":"monument_historique"},{"nom_mon":"Immeuble","adresse_mon":"5 place Bellecour","commune_mon":"Lyon","historique":null,"id_mon":"PA00117826","siecle":null,"lat_mon":"45.756681","lng_mon":"4.831715","type":"monument_historique"},{"nom_mon":"Immeuble","adresse_mon":"place Bellecour ","commune_mon":"Lyon","historique":null,"id_mon":"PA00117846","siecle":null,"lat_mon":"45.634884","lng_mon":"4.38167","type":"monument_historique"},{"nom_mon":"Edifice gallo-romain de forme rectangulaire","adresse_mon":null,"commune_mon":"Lyon","historique":null,"id_mon":"PA00117793","siecle":2,"lat_mon":"45.7611193596","lng_mon":"4.81655483631","type":"monument_historique"},{"nom_mon":"Maison","adresse_mon":"19 rue du Boeuf","commune_mon":"Lyon","historique":null,"id_mon":"PA00117887","siecle":null,"lat_mon":"45.7624019851","lng_mon":"4.82689762023","type":"monument_historique"},{"nom_mon":"Fontaine du Taurobole","adresse_mon":"rue des Macchab\u00e9es","commune_mon":"Lyon","historique":null,"id_mon":"PA00117808","siecle":19,"lat_mon":"45.7557661668","lng_mon":"4.81655089658","type":"monument_historique"},{"nom_mon":"H\u00f4pital militaire Villemanzy","adresse_